{
  "name": "better-auth-legal-consent",
  "version": "1.0.6",
  "description": "Keep your application compliant by requiring legal consent on your sign-up routes.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ping-maxwell/better-auth-kit"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun build --entrypoints ./src/index.ts --outdir ./dist --target node --format esm --sourcemap=external --minify --packages external --watch",
    "build": "bun build.ts && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --outDir dist && echo \"completed type generation.\""
  },
  "keywords": ["better-auth", "legal-consent", "plugin"],
  "author": "ping-maxwell",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "better-auth": "^1.1.21"
  }
}
