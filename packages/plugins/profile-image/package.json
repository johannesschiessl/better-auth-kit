{
	"name": "@better-auth-kit/profile-image",
	"version": "0.1.0",
	"description": "Profile image upload plugin for Better Auth Kit",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"better-auth",
		"auth",
		"plugin",
		"profile",
		"image",
		"avatar",
		"upload"
	],
	"license": "MIT",
	"author": "better-auth-kit",
	"files": ["./dist/**/*"],
	"scripts": {
		"build": "bun build.ts",
		"build:dev": "bun build-dev.ts",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"zod": "^3.24.2",
		"uploadthing": "^6.13.3",
		"@uploadthing/shared": "^6.7.9"
	},
	"peerDependencies": {
		"better-auth": "^1.1.21"
	},
	"devDependencies": {
		"@better-auth-kit/internal-build": "workspace:*",
		"vitest": "^3.0.8",
		"@better-auth-kit/tests": "workspace:*"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"default": "./dist/client.js"
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
